{
  "name": "teaching-server",
  "version": "0.0.1",
  "description": "the teaching server.",
  "keywords": [
    "teaching",
    "server"
  ],
  "main": "dist/server.js",
  "typings": "dist/types/server.d.ts",
  "files": [
    "dist"
  ],
  "author": "GaoYakun <g.leisuregame@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/gaoyakun/teaching-server"
  },
  "license": "GPL-3.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "lint": "npm run tslint && npm run prettier",
    "tslint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prettier": "prettier --write --single-quote --list-different --tab-width 4 --print-width 180",
    "make-proto-list": "pbjs -t json -o src/common/protocols/protocols.json protocols/*.proto && node scripts/build-proto-list.js src/common/protocols/protocols.json src/common/protocols/protolist.ts",
    "make-proto-js": "pbjs -t static-module -w commonjs -o src/common/protocols/protocols.js protocols/*.proto",
    "make-proto-ts": "pbts -o src/common/protocols/protocols.d.ts src/common/protocols/protocols.js",
    "make-proto": "npm run make-proto-js && npm run make-proto-ts && npm run make-proto-list",
    "build": "npm run clean && npm run make-proto && tsc -p src/webserver && tsc -p src/roomserver && tsc -p src/site && node scripts/build-site.js && shx cp -R src/webserver/install dist/webserver/webserver && shx cp -R src/webserver/views dist/webserver/webserver",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "start-server": "npm run build && node dist/webserver/webserver/main.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/scripts/prep.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": false,
    "mapCoverage": false
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/jest": "^21.1.0",
    "@types/node": "^8.10.38",
    "colors": "^1.1.2",
    "commitizen": "^2.9.6",
    "commitlint": "^7.2.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.1",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^5.10.0",
    "husky": "^1.2.0",
    "jest": "^21.0.0",
    "prettier": "^1.15.3",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.0.0-beta.2",
    "rimraf": "^2.6.1",
    "semantic-release": "^8.2.0",
    "shx": "^0.3.2",
    "ts-jest": "^21.0.0",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^6.0.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.3.4",
    "validate-commit-msg": "^2.12.2"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/express-fileupload": "^0.4.0",
    "@types/http-errors": "^1.6.1",
    "@types/jquery": "^3.3.27",
    "@types/jstree": "^3.3.37",
    "@types/lodash": "^4.14.108",
    "@types/long": "^4.0.0",
    "@types/morgan": "^1.7.35",
    "@types/mysql": "^2.15.5",
    "@types/protobufjs": "^6.0.0",
    "@types/redis": "^2.8.8",
    "@types/sharp": "^0.21.0",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "cookie-parser": "^1.4.3",
    "cssnano": "^4.1.7",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.0.0",
    "http-errors": "^1.7.1",
    "jquery": "^3.3.1",
    "jstree": "^3.3.7",
    "long": "^4.0.0",
    "make-error": "^1.3.0",
    "morgan": "^1.9.1",
    "mysql": "^2.16.0",
    "popper.js": "^1.14.6",
    "postcss-cssnext": "^3.1.0",
    "postcss-nested": "^4.1.0",
    "postcss-simple-vars": "^5.0.1",
    "protobufjs": "^6.8.8",
    "redis": "^2.8.0",
    "rollup": "^0.67.4",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-postcss": "^1.6.3",
    "rollup-plugin-uglify": "^6.0.0",
    "sharp": "^0.21.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0"
  }
}
